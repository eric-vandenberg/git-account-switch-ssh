{
  "name": "git-account-switch-ssh",
  "version": "1.0.0",
  "description": "Git Account Switch (SSH)",
  "main": "./dist/cli.js",
  "scripts": {
    "start": "jiti ./lib/cli.ts",
    "build": "tsup ./lib/",
    "local": "sudo npm i -g && gas",
    "reset": "rm -rf ./node_modules ./yarn.lock && yarn"
  },
  "bin": {
    "gas": "./dist/cli.js"
  },
  "keywords": [
    "cli",
    "git",
    "ssh",
    "npmtool"
  ],
  "author": "Eric Vandenberg <contact@ericvandenberg.com>",
  "license": "MIT",
  "repository": "https://github.com/Eric-Vandenberg/git-account-switch-ssh",
  "homepage": "https://github.com/Eric-Vandenberg/git-account-switch-ssh#readme",
  "dependencies": {
    "@clack/core": "^0.3.4",
    "@clack/prompts": "^0.7.0",
    "figlet": "^1.7.0",
    "lolcatjs": "^2.4.3",
    "picocolors": "^1.0.0",
    "ssh-config": "^4.4.2"
  },
  "devDependencies": {
    "@types/figlet": "^1.5.8",
    "@types/node": "^20.12.2",
    "jiti": "^1.21.0",
    "tsup": "^8.0.2",
    "typescript": "^5.4.3"
  }
}
