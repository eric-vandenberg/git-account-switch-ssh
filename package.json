{
  "name": "git-account-switch-ssh",
  "version": "1.1.0",
  "description": "Git Account Switch (SSH)",
  "main": "./dist/cli.js",
  "scripts": {
    "start": "jiti ./lib/cli.ts",
    "build": "tsup",
    "local": "npm run build && sudo npm i -g",
    "reset": "rm -rf ./node_modules ./dist ./package-lock.json && npm i"
  },
  "bin": {
    "gas": "./dist/cli.js"
  },
  "keywords": [
    "cli",
    "command-line",
    "git",
    "github",
    "gitlab",
    "npmtool",
    "ssh",
    "ssh-add",
    "ssh-agent",
    "ssh-config",
    "ssh-keyscan",
    "terminal"
  ],
  "author": "Eric Vandenberg (contact@ericvandenberg.com)",
  "license": "MIT",
  "homepage": "https://github.com/eric-vandenberg/git-account-switch-ssh#readme",
  "dependencies": {
    "@clack/core": "^0.3.4",
    "@clack/prompts": "^0.7.0",
    "chalk": "^4.1.2",
    "gradient-string": "^2.0.2",
    "ssh-config": "^4.4.2"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@types/gradient-string": "^1.1.6",
    "@types/node": "^20.12.2",
    "jiti": "^1.21.0",
    "semantic-release": "^23.0.8",
    "tsup": "^8.0.2",
    "typescript": "^5.4.3"
  },
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/changelog",
        {
          "changelogFile": "CHANGELOG.md"
        }
      ],
      "@semantic-release/npm",
      "@semantic-release/github",
      [
        "@semantic-release/git",
        {
          "assets": [
            "package.json",
            "CHANGELOG.md"
          ],
          "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ]
    ]
  }
}
