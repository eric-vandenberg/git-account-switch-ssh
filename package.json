{
  "name": "git-account-switch-ssh",
  "version": "1.0.0",
  "description": "Git Account Switch (SSH)",
  "main": "dist/cli.js",
  "scripts": {
    "start": "jiti ./lib/cli.ts",
    "build": "tsup ./lib/",
    "local": "yarn build && sudo npm i -g",
    "reset": "rm -rf ./node_modules ./dist ./yarn.lock && yarn"
  },
  "bin": {
    "gas": "./dist/cli.js"
  },
  "keywords": [
    "cli",
    "command-line",
    "git",
    "github",
    "gitlab",
    "npmtool",
    "ssh",
    "ssh-add",
    "ssh-agent",
    "ssh-config",
    "ssh-keyscan",
    "terminal"
  ],
  "author": "Eric Vandenberg (contact@ericvandenberg.com)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:eric-vandenberg/git-account-switch-ssh.git"
  },
  "homepage": "https://github.com/Eric-Vandenberg/git-account-switch-ssh#readme",
  "dependencies": {
    "@clack/core": "^0.3.4",
    "@clack/prompts": "^0.7.0",
    "chalk": "^4.1.2",
    "gradient-string": "^2.0.2",
    "ssh-config": "^4.4.2"
  },
  "devDependencies": {
    "@types/gradient-string": "^1.1.6",
    "@types/node": "^20.12.2",
    "jiti": "^1.21.0",
    "tsup": "^8.0.2",
    "typescript": "^5.4.3"
  }
}
